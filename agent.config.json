{
  "active_provider": "ollama",
  "providers": {
    "ollama": {
      "endpoint": "localhost:11434",
      "model": "qwen3:14b",
      "generation": {
        "temperature": 0.7,
        "num_ctx": 131072
      }
    },
    "openai": {
      "model": "gpt-4o",
      "generation": {
        "temperature": 0
      }
    },
    "anthropic": {
      "model": "claude-3-5-sonnet-20241022",
      "generation": {
        "temperature": 0,
        "max_tokens": 4096
      }
    },
    "gemini": {
      "model": "gemini-2.0-flash-exp",
      "generation": {
        "temperature": 0,
        "max_output_tokens": 1000
      }
    }
  },
  "run_policy": "ask",
  "fast_mode": true,
  "voice_mode": false,
  "newline_support": true,
  "visibility_allowed": false,
  "auto_reload_session": false,
  "web_browsing_allowed": true,
  "planning_mode": false,
  "max_budget": 25,
  "mcp_enabled": true,
  "mcp_servers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:\\path\\to\\allowed\\files"
      ]
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "everything": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "YOUR_API_KEY_HERE"
      }
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_PAT_HERE"
      }
    }
  },
  "theme": {
    "primary": "gray",
    "secondary": "magenta",
    "accent": "cyan",
    "success": "green",
    "warning": "yellow",
    "error": "red",
    "dim": "gray",
    "bg": "default"
  },
  "think_mode": true,
  "see_project_mode": false,
  "effort_level": "medium",
  "reasoning_level": "high",
  "stream": true,
  "stream_print": true,
  "stream_timeout_ms": 90000,
  "stream_retry_count": 1,
  "stream_render_fps": 24,
  "command_timeout_ms": 30000,
  "command_log_enabled": true,
  "env_bridge_enabled": true,
  "mission_mode": false,
  "include_history": false,
  "theme_mode": "dark",
  "access_scope": "limited",
  "permissions_mode": "limited",
  "permission_mode": "limited",
  "permissions": {
    "mode": "limited",
    "read": "allow",
    "write": "ask",
    "execute": "ask",
    "web": "ask"
  }
}